<% content_for :title, "Editing City" %>

<div class="container mt-4">
  <h1 class="mb-4 text-primary">Editing City</h1>

    <%= render "form", branch: @branch %>

  <br>

  <div class="mt-3">
    <%= link_to "Show this City", @branch, class: "btn btn-info me-2" %>
    <%= link_to "Back to cities", branches_path, class: "btn btn-primary" %>
    <%= link_to "Cancel", "javascript:history.back()", class: "btn btn-danger ms-2" %>
  </div>
</div>
